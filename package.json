		"react-rnd": "^10.3.7",
		"react-router-dom": "^5.3.4",
		"regenerator-runtime": "^0.13.11",
		"tslib": "^2.4.1",
		"turbo": "^1.6.3",
		"typescript": "^4.9.4"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie 11",
		"not op_mini all"
