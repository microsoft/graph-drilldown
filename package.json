{
	"name": "@graph-drilldown/project",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"clean": "turbo run clean --parallel",
		"build": "turbo run build",
		"bundle": "turbo run bundle",
		"start": "turbo run start",
		"lint": "turbo run lint",
		"ci": "turbo run ci",
		"prettify": "essex prettify",
		"deploy": "essex deploy --type azure-blob-storage --storageAccount $STORAGE_ACCOUNT --storageAccountKey $STORAGE_KEY --verbose",
		"is_clean": "git diff-index HEAD --quiet || git status",
		"update_sdks": "yarn dlx @yarnpkg/sdks vscode"
	},
	"workspaces": [
		"packages/*"
	],
	"devDependencies": {
		"@essex/eslint-config": "^20.3.5",
		"@essex/eslint-plugin": "^20.3.12",
		"@essex/jest-config": "^21.0.17",
		"@essex/prettier-config": "^18.0.4",
		"@essex/scripts": "^22.0.4",
		"@fluentui/font-icons-mdl2": "^8.5.4",
		"@fluentui/react": "^8.103.9",
		"@fluentui/react-hooks": "^8.6.14",
		"@fluentui/utilities": "^8.13.4",
		"@types/jest": "^29.2.4",
		"@types/node": "^18.11.17",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@types/react-rnd": "^8.0.0",
		"@types/react-router-dom": "^5.3.3",
		"core-js": "^3.26.1",
		"eslint": "^8.28.0",
